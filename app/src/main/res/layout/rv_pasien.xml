<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvNama"
        android:layout_width="38dp"
        android:layout_height="23dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="10dp"
        android:text="A"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labelNama" />

    <Button
        android:id="@+id/btn"
        android:layout_width="64dp"
        android:layout_height="35dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="32dp"
        android:text="Lihat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/labelAksi" />

    <TextView
        android:id="@+id/labelHasil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:text="Hasil Diagnosa"
        app:layout_constraintStart_toEndOf="@+id/labelNama"
        tools:layout_editor_absoluteY="70dp" />

    <TextView
        android:id="@+id/labelNama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="Nama"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.125"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="70dp" />

    <TextView
        android:id="@+id/tvHasil"
        android:layout_width="18dp"
        android:layout_height="24dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="10dp"
        android:text="B"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/btn"
        app:layout_constraintHorizontal_bias="0.254"
        app:layout_constraintStart_toEndOf="@+id/tvNama"
        app:layout_constraintTop_toBottomOf="@+id/labelHasil" />

    <TextView
        android:id="@+id/labelAksi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aksi"
        tools:layout_editor_absoluteX="318dp"
        tools:layout_editor_absoluteY="67dp" />

</androidx.constraintlayout.widget.ConstraintLayout>